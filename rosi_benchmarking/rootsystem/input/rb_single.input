[Problem]
Name = rb_single

[TimeLoop]
TEnd = 1.e9 # 1e9 # [s]
DtInitial = 1 # [s]

[RootSystem.Grid]
File =  singleroot # Triticum_aestivum_a_Bingham_2011  # Anagallis_femina_Leitner_2010
InitialT = 50 # days

[RootSystem.Conductivity]
Kr = 1.728 # radial conductivity [cm/hPa/day] 
Kx = 432 # axial conductivity [cm^4/hPa/day]

[RootSystem.Collar]
P = -1000 # cm pressure head

[Soil.IC]
P = -200 # cm pressure head

[Vtk]
AddProcessRank = "false"
AddVelocity = "true"
