[Problem]
Name = benchmark1 

[TimeLoop]
TEnd = 1e9 # 1e9 # [s]
DtInitial = 1 # [s]

[RootSystem.Grid]
File =  grids/singleroot.dgf

[RootSystem.Conductivity]
Kr = 2.e-9  # radial conductivity (m^2 s / kg) 
Kx = 5.e-13 # axial conductivity (m^5 s / kg)  

[RootSystem]
Radius = 0.002 # m

[RootSystem.Collar]
P = -1000 # cm pressure head

[Soil.IC]
P = -200 # cm pressure head

[Vtk]
AddProcessRank = "false"
AddVelocity = "true"
