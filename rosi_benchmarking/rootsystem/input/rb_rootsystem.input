[Problem]
Name = rb_rootsystem 

[TimeLoop]
TEnd = 0 # 1e9 # [s]
DtInitial = 1 # [s]

[RootSystem.Grid]
File =  Triticum_aestivum_a_Bingham_2011 
# Anagallis_femina_Leitner_2010
InitialT = 10 # days

[RootSystem.Conductivity]
Kr = 2.e-13  # radial conductivity (m^2 s / kg) 
Kx = 5.e-14 # axial conductivity (m^5 s / kg)  

[RootSystem.Collar]
P = -1000 # cm pressure head

[RootSystem.Soil]
P = -100 # cm pressure head

[Vtk]
AddProcessRank = "false"
AddVelocity = "true"
